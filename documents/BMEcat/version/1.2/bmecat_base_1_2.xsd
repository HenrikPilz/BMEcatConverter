				<xsd:element ref="MIME_ORDER" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="MIME_ALT">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MIME_DESCR">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="250"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MIME_INFO">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="MIME" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="MIME_ORDER" type="dtINTEGER"/>
	<xsd:element name="MIME_PURPOSE">
		<xsd:simpleType>
			<xsd:restriction base="xsd:NMTOKEN">
				<xsd:enumeration value="thumbnail"/>
				<xsd:enumeration value="normal"/>
				<xsd:enumeration value="detail"/>
				<xsd:enumeration value="data_sheet"/>
				<xsd:enumeration value="logo"/>
				<xsd:enumeration value="others"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MIME_ROOT">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="100"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MIME_SOURCE">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="250"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="MIME_TYPE">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="30"/>
				<xsd:minLength value="1"/>
				<!-- ***** predefined values for element 'MIME_TYPE' *****
					"(url | application/pdf | image/jpeg | image/gif | text/html | text/plain | ...)" 
				-->
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="NAME">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="NAME2">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="NAME3">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="NO_CU_PER_OU" type="dtNUMBER"/>
	<xsd:element name="ORDER_UNIT" type="dtPUNIT"/>
	<xsd:element name="PARENT_ID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="PHONE">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="30"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="PRICE_AMOUNT" type="dtNUMBER"/>
	<xsd:element name="PRICE_CURRENCY" type="dtCURRENCIES"/>
	<xsd:element name="PRICE_FACTOR" type="dtNUMBER"/>
	<xsd:element name="PRICE_FLAG">
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="dtBOOLEAN">
					<xsd:attribute name="type" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:NMTOKEN">
								<xsd:enumeration value="incl_freight"/>
								<xsd:enumeration value="incl_duty"/>
								<xsd:enumeration value="incl_packing"/>
								<xsd:enumeration value="incl_assurance"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="PRICE_QUANTITY" type="dtNUMBER"/>
	<xsd:element name="PUBLIC_KEY">
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="typePUBLIC_KEY">
					<xsd:attribute name="type" use="required">
						<xsd:simpleType>
							<xsd:restriction base="dtSTRING">
								<xsd:maxLength value="50"/>
								<xsd:minLength value="1"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="QUANTITY_INTERVAL" type="dtINTEGER"/>
	<xsd:element name="QUANTITY_MIN" type="dtINTEGER"/>
	<xsd:element name="REFERENCE_FEATURE_GROUP_ID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="60"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="REFERENCE_FEATURE_GROUP_NAME">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="60"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="REFERENCE_FEATURE_SYSTEM_NAME">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
				<!-- ***** predefined values for element 'REFERENCE_FEATURE_SYSTEM_NAME' *****
					"(ECLASS-x.y | ETIM-x.y | UNSPSC-x.y | udf_ZZZ-x.y)" 
				-->
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="REMARKS">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:minLength value="1"/>
				<xsd:maxLength value="64000"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="SEGMENT">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="100"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="SPECIAL_TREATMENT_CLASS">
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="typeSPECIAL_TREATMENT_CLASS">
					<xsd:attribute name="type" use="required">
						<xsd:simpleType>
							<xsd:restriction base="dtSTRING">
								<xsd:maxLength value="50"/>
								<xsd:minLength value="1"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="STATE">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="STREET">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="SUPPLIER">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="SUPPLIER_ID" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="SUPPLIER_NAME"/>
				<xsd:element name="ADDRESS" minOccurs="0">
					<xsd:complexType>
						<xsd:complexContent>
							<xsd:extension base="typeADDRESS">
								<xsd:attribute name="type" fixed="supplier"/>
							</xsd:extension>
						</xsd:complexContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="MIME_INFO" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
		<!-- ***** START deactivate identity-constraints ***** -->
		<xsd:unique name="uniqueSUPPLIER_IDtype">
			<xsd:selector xpath="SUPPLIER_ID"/>
			<xsd:field xpath="@type"/>
		</xsd:unique>
		<!-- ***** END deactivate identity-constraints   ***** -->
	</xsd:element>
	<xsd:element name="SUPPLIER_AID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="32"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="SUPPLIER_AID_SUPPLEMENT">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="31"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="SUPPLIER_ALT_AID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="SUPPLIER_ID">
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="typeSUPPLIER_ID">
					<xsd:attribute name="type" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="dtSTRING">
								<xsd:maxLength value="50"/>
								<xsd:minLength value="1"/>
								<!-- ***** predefined values for attribute 'type' of element 'SUPPLIER_ID' *****
									"(duns | iln | buyer_specific | supplier_specific)" 
								-->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SUPPLIER_NAME">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="50"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="SYNONYM">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="60"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="TAX" type="dtNUMBER"/>
	<xsd:element name="TERRITORY" type="dtCOUNTRIES"/>
	<xsd:element name="TIME" type="dtTIMETYPE"/>
	<xsd:element name="TIMEZONE" type="dtTIMEZONETYPE"/>
	<xsd:element name="UNIT">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="UNIT_ID"/>
				<xsd:element ref="UNIT_NAME" minOccurs="0"/>
				<xsd:element ref="UNIT_DESCR" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="system" use="optional">
				<xsd:simpleType>
					<xsd:restriction base="dtSTRING">
						<xsd:maxLength value="20"/>
						<xsd:minLength value="1"/>
						<!-- ***** predefined values for attribute 'type' of element 'SUPPLIER_ID' *****
							"(unece | si)" 
						-->
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="UNITS">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="UNIT" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="UNIT_DESCR">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="250"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="UNIT_ID">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="60"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="UNIT_NAME">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="60"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="URL">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="100"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="VARIANT">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="FVALUE"/>
				<xsd:element ref="SUPPLIER_AID_SUPPLEMENT"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="VARIANTS">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="VARIANT" maxOccurs="unbounded"/>
				<xsd:element ref="VORDER"/>
			</xsd:sequence>
		</xsd:complexType>
		<!-- ***** START deactivate identity-constraints ***** -->
		<xsd:key name="keySUPPLIER_AID_SUPPLEMENT">
			<xsd:selector xpath="VARIANT"/>
			<xsd:field xpath="SUPPLIER_AID_SUPPLEMENT"/>
		</xsd:key>
		<!-- ***** END deactivate identity-constraints   ***** -->
	</xsd:element>
	<xsd:element name="VORDER" type="dtINTEGER"/>
	<xsd:element name="ZIP">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="20"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="ZIPBOX">
		<xsd:simpleType>
			<xsd:restriction base="dtSTRING">
				<xsd:maxLength value="20"/>
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
</xsd:schema>
