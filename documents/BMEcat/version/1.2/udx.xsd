<!-- 
	Example-include-file for USER_DEFINED_EXTENSIONS
	- ARTICLE
	- HEADER
	- CATALOG_GROUP_STRUCTURE

	complexType's have to be listed even if they are not used (they can be empty)

	Beispiel-Include-Datei fuer USER_DEFINED_EXTENSIONS
	- ARTICLE
	- HEADER
	- CATALOG_GROUP_STRUCTURE
	
	Die complexType's muessen vorhanden sein, auch wenn sie nicht benutzt werden 
	(koennen dann leer bleiben).
-->

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
		elementFormDefault="qualified">

	<xsd:complexType name="headerUSER_DEFINED_EXTENSIONS"/>
	<xsd:complexType name="catstrucUSER_DEFINED_EXTENSIONS"/>
	<xsd:complexType name="articleUSER_DEFINED_EXTENSIONS"/>
<!-- 
	Example:
	<xsd:complexType name="articleUSER_DEFINED_EXTENSIONS">
		<xsd:choice>
			<xsd:element name="udx.BasePUnit" type="xsd:string"/>
			<xsd:element name="udx.SecurityLevel" type="xsd:integer"/>
		</xsd:choice>
	</xsd:complexType>
-->
</xsd:schema>
