        <xsd:maxLength value="50"/>
        <xsd:pattern value="ean|gtin|upc|\w{1,50}"/>
       </xsd:restriction>
      </xsd:simpleType>
     </xsd:attribute>
    </xsd:extension>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="KEYWORD">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:maxLength value="50"/>
     <xsd:minLength value="1"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="LANGUAGE">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:extension base="dtLANG">
     <xsd:attribute name="default" type="dtBOOLEAN" use="optional"/>
    </xsd:extension>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="LEADTIME" type="dtFLOAT"/>
 <xsd:element name="LENGTH" type="dtNUMBER"/>
 <xsd:element name="LOWER_BOUND" type="dtNUMBER"/>
 <xsd:element name="MANUFACTURER_NAME">
  <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:maxLength value="50"/>
    <xsd:minLength value="1"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="MANUFACTURER_PID">
  <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:maxLength value="50"/>
    <xsd:minLength value="1"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="MANUFACTURER_TYPE_DESCR">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:maxLength value="50"/>
     <xsd:minLength value="1"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="MAX_OCCURANCE" type="dtCOUNT"/>
 <xsd:element name="MIME">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element ref="MIME_TYPE"/>
    <xsd:element ref="MIME_SOURCE" maxOccurs="unbounded"/>
    <xsd:element ref="MIME_DESCR" minOccurs="0" maxOccurs="unbounded"/>
    <xsd:element ref="MIME_ALT" minOccurs="0" maxOccurs="unbounded"/>
    <xsd:element ref="MIME_PURPOSE"/>
   </xsd:sequence>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="MIME_ALT">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:maxLength value="80"/>
     <xsd:minLength value="1"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="MIME_DESCR">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:maxLength value="250"/>
     <xsd:minLength value="1"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="MIME_INFO">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element ref="MIME" maxOccurs="unbounded"/>
   </xsd:sequence>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="MIME_PURPOSE">
  <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:minLength value="1"/>
    <xsd:maxLength value="20"/>
    <xsd:enumeration value="data_sheet"/>
    <xsd:enumeration value="detail"/>
    <xsd:enumeration value="icon"/>
    <xsd:enumeration value="logo"/>
    <xsd:enumeration value="normal"/>
    <xsd:enumeration value="safety_data_sheet"/>
    <xsd:enumeration value="thumbnail"/>
    <xsd:enumeration value="others"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="MIME_ROOT">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:maxLength value="250"/>
     <xsd:minLength value="1"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="MIME_SOURCE">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:maxLength value="255"/>
     <xsd:minLength value="1"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="MIME_TYPE">
  <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:maxLength value="30"/>
    <xsd:minLength value="1"/>
    <xsd:pattern value="application/pdf|application/xml|image/gif|image/jpeg|text/html|text/plain|url|[\w\-\.]{1,30}"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="MIN_OCCURANCE" type="dtCOUNT"/>
 <xsd:element name="NAME">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:maxLength value="50"/>
     <xsd:minLength value="1"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="NAME2">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:maxLength value="50"/>
     <xsd:minLength value="1"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="NAME3">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:maxLength value="50"/>
     <xsd:minLength value="1"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="NO_CU_PER_OU" type="dtNUMBER" default="1"/>
 <xsd:element name="ORDER_UNIT" type="dtPUNIT"/>
 <xsd:element name="PHONE">
  <xsd:complexType>
   <xsd:complexContent>
    <xsd:extension base="typeMLSTRING00050">
     <xsd:attribute name="type" use="optional">
      <xsd:simpleType>
       <xsd:restriction base="dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="50"/>
        <xsd:pattern value="mobile|office|private|\w{1,50}"/>
       </xsd:restriction>
      </xsd:simpleType>
     </xsd:attribute>
    </xsd:extension>
   </xsd:complexContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="PRICE_AMOUNT" type="dtNUMBER"/>
 <xsd:element name="PRICE_BASE">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element ref="PRICE_UNIT"/>
    <xsd:element ref="PRICE_UNIT_FACTOR" minOccurs="0"/>
   </xsd:sequence>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="PRICE_CURRENCY" type="dtCURRENCIES"/>
 <xsd:element name="PRICE_FACTOR" type="dtNUMBER" default="1"/>
 <xsd:element name="PRICE_FLAG">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:extension base="dtBOOLEAN">
     <xsd:attribute name="type" use="required">
      <xsd:simpleType>
       <xsd:restriction base="dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="20"/>
        <xsd:pattern value="incl_assurance|incl_duty|incl_freight|incl_insurance|incl_packing|\w{1,20}"/>
       </xsd:restriction>
      </xsd:simpleType>
     </xsd:attribute>
    </xsd:extension>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="PRICE_QUANTITY" type="dtNUMBER" default="1"/>
 <xsd:element name="PRICE_TYPE" type="typePRICE_TYPE"/>
 <xsd:element name="PRICE_UNIT" type="dtPUNIT"/>
 <xsd:element name="PRICE_UNIT_FACTOR" type="dtFLOAT" default="1"/>
 <xsd:element name="PROD_ID" type="typeSUPPLIER_PID"/>
 <xsd:element name="PROD_ID_TO">
  <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:maxLength value="80"/>
    <xsd:minLength value="1"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="PRODUCT_CATEGORY">
  <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:minLength value="1"/>
    <xsd:maxLength value="20"/>
    <xsd:enumeration value="consignment"/>
    <xsd:enumeration value="core_product"/>
    <xsd:enumeration value="preferred"/>
    <xsd:enumeration value="standard"/>
    <xsd:enumeration value="stock"/>
    <xsd:enumeration value="others"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="PRODUCT_DETAILS">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element ref="DESCRIPTION_SHORT" maxOccurs="unbounded"/>
    <xsd:element ref="DESCRIPTION_LONG" minOccurs="0" maxOccurs="unbounded"/>
    <xsd:element ref="INTERNATIONAL_PID" minOccurs="0" maxOccurs="unbounded"/>
    <xsd:element ref="SUPPLIER_ALT_PID" minOccurs="0"/>
    <xsd:element ref="BUYER_PID" minOccurs="0" maxOccurs="unbounded"/>
    <xsd:element ref="MANUFACTURER_PID" minOccurs="0"/>
    <xsd:element ref="MANUFACTURER_NAME" minOccurs="0"/>
    <xsd:element ref="MANUFACTURER_TYPE_DESCR" minOccurs="0" maxOccurs="unbounded"/>
    <xsd:element ref="SPECIAL_TREATMENT_CLASS" minOccurs="0" maxOccurs="unbounded"/>
    <xsd:element ref="KEYWORD" minOccurs="0" maxOccurs="unbounded"/>
    <xsd:element ref="REMARKS" minOccurs="0" maxOccurs="unbounded"/>
    <xsd:element ref="PRODUCT_STATUS" minOccurs="0" maxOccurs="unbounded"/>
   </xsd:sequence>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="PRODUCT_DIMENSIONS">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element ref="VOLUME" minOccurs="0"/>
    <xsd:element ref="WEIGHT" minOccurs="0"/>
    <xsd:element ref="LENGTH" minOccurs="0"/>
    <xsd:element ref="WIDTH" minOccurs="0"/>
    <xsd:element ref="DEPTH" minOccurs="0"/>
   </xsd:sequence>
  </xsd:complexType>
 </xsd:element> 
 <xsd:element name="PRODUCT_FEATURES">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element ref="REFERENCE_FEATURE_SYSTEM_NAME"/>
    <xsd:element ref="REFERENCE_FEATURE_GROUP_ID"/>
    <xsd:element ref="FEATURE" maxOccurs="unbounded"/>
   </xsd:sequence>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="PRODUCT_LOGISTIC_DETAILS">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element ref="CUSTOMS_TARIFF_NUMBER" minOccurs="0" maxOccurs="unbounded"/>
    <xsd:element ref="STATISTICS_FACTOR" minOccurs="0"/>
    <xsd:element ref="COUNTRY_OF_ORIGIN" minOccurs="0" maxOccurs="unbounded"/>
    <xsd:element ref="PRODUCT_DIMENSIONS" minOccurs="0"/>
   </xsd:sequence>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="PRODUCT_ORDER_DETAILS">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element ref="ORDER_UNIT"/>
    <xsd:element ref="CONTENT_UNIT"/>
    <xsd:element ref="NO_CU_PER_OU" minOccurs="0"/>
    <xsd:element ref="PRICE_QUANTITY" minOccurs="0"/>
    <xsd:element ref="QUANTITY_MIN" minOccurs="0"/>
    <xsd:element ref="QUANTITY_INTERVAL" minOccurs="0"/>
   </xsd:sequence>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="PRODUCT_PRICE">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element ref="PRICE_AMOUNT"/>
    <xsd:element ref="PRICE_CURRENCY" minOccurs="0"/>
    <xsd:element ref="TAX" minOccurs="0"/>
    <xsd:element ref="PRICE_FACTOR" minOccurs="0"/>
    <xsd:element ref="LOWER_BOUND" minOccurs="0"/>
    <xsd:element ref="TERRITORY" minOccurs="0" maxOccurs="unbounded"/>
   </xsd:sequence>
   <xsd:attribute name="price_type" type="typePRICE_TYPE" use="required"/>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="PRODUCT_PRICE_DETAILS">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element name="DATETIME" maxOccurs="2">
     <xsd:complexType>
      <xsd:complexContent>
       <xsd:extension base="typeDATETIME">
        <xsd:attribute name="type" use="required">
         <xsd:simpleType>
          <xsd:restriction base="dtSTRING">
           <xsd:minLength value="1"/>
           <xsd:maxLength value="20"/>
           <xsd:enumeration value="valid_start_date"/>
           <xsd:enumeration value="valid_end_date"/>
          </xsd:restriction>
         </xsd:simpleType>
        </xsd:attribute>
       </xsd:extension>
      </xsd:complexContent>
     </xsd:complexType>
    </xsd:element>
    <xsd:element ref="DAILY_PRICE" minOccurs="0"/>
    <xsd:element ref="PRODUCT_PRICE" maxOccurs="unbounded"/>
   </xsd:sequence>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="PRODUCT_REFERENCE">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element ref="PROD_ID_TO"/>
    <xsd:element ref="CATALOG_ID" minOccurs="0"/>
    <xsd:element ref="CATALOG_VERSION" minOccurs="0"/>
   </xsd:sequence>
   <xsd:attribute name="type" use="required">
    <xsd:simpleType>
     <xsd:restriction base="dtSTRING">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="20"/>
      <xsd:enumeration value="accessories"/>
      <xsd:enumeration value="base_product"/>
      <xsd:enumeration value="consists_of"/>
      <xsd:enumeration value="diff_orderunit"/>
      <xsd:enumeration value="followup"/>
      <xsd:enumeration value="mandatory"/>
      <xsd:enumeration value="similar"/>
      <xsd:enumeration value="select"/>
      <xsd:enumeration value="sparepart"/>
      <xsd:enumeration value="others"/>
     </xsd:restriction>
    </xsd:simpleType>
   </xsd:attribute>
   <xsd:attribute name="quantity" type="dtINTEGER" use="optional"/>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="PRODUCT_STATUS">
  <xsd:complexType>
   <xsd:complexContent>
    <xsd:extension base="typeMLSTRING00250">
     <xsd:attribute name="type" use="required">
      <xsd:simpleType>
       <xsd:restriction base="dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="20"/>
        <xsd:enumeration value="bargain"/>
        <xsd:enumeration value="core_product"/>
        <xsd:enumeration value="new"/>
        <xsd:enumeration value="new_product"/>
        <xsd:enumeration value="old_product"/>
        <xsd:enumeration value="refurbished"/>
        <xsd:enumeration value="used"/>
        <xsd:enumeration value="others"/>
       </xsd:restriction>
      </xsd:simpleType>
     </xsd:attribute>
    </xsd:extension>
   </xsd:complexContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="QUANTITY_INTERVAL" type="dtFLOAT" default="1"/>
 <xsd:element name="QUANTITY_MIN" type="dtFLOAT" default="1"/>
 <xsd:element name="REFERENCE_DESCR">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:maxLength value="250"/>
     <xsd:minLength value="1"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="REFERENCE_FEATURE_GROUP_ID" type="typeCLASSIFICATION_GROUP_ID"/>
 <xsd:element name="REFERENCE_FEATURE_SYSTEM_NAME" type="typeCLASSIFICATION_SYSTEM_NAME"/>
 <xsd:element name="REMARKS">
  <xsd:complexType>
   <xsd:complexContent>
    <xsd:extension base="typeMLSTRING64000">
     <xsd:attribute name="type" use="optional">
      <xsd:simpleType>
       <xsd:restriction base="dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="250"/>
        <xsd:pattern value="deliverynote|dispatchnotification|general|invoice|order|orderchange|orderresponse|quotation|receiptacknowledgement|rfq|transport|\w{1,250}"/>
       </xsd:restriction>
      </xsd:simpleType>
     </xsd:attribute>
    </xsd:extension>
   </xsd:complexContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="SPECIAL_TREATMENT_CLASS">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:extension base="typeSTRING00020">
     <xsd:attribute name="type" use="required">
      <xsd:simpleType>
       <xsd:restriction base="dtSTRING">
        <xsd:maxLength value="50"/>
        <xsd:minLength value="1"/>
       </xsd:restriction>
      </xsd:simpleType>
     </xsd:attribute>
    </xsd:extension>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="STATE">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:maxLength value="50"/>
     <xsd:minLength value="1"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="STATISTICS_FACTOR" type="dtNUMBER"/>
 <xsd:element name="STREET">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:maxLength value="50"/>
     <xsd:minLength value="1"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="SUPPLIER">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element ref="SUPPLIER_ID" minOccurs="0" maxOccurs="unbounded"/>
    <xsd:element ref="SUPPLIER_NAME"/>
    <xsd:element name="ADDRESS" minOccurs="0">
     <xsd:complexType>
      <xsd:complexContent>
       <xsd:extension base="typeADDRESS">
        <xsd:attribute name="type" use="required" fixed="supplier">
         <xsd:simpleType>
          <xsd:restriction base="dtSTRING">
           <xsd:minLength value="1"/>
           <xsd:maxLength value="20"/>
           <xsd:enumeration value="supplier"/>
          </xsd:restriction>
         </xsd:simpleType>
        </xsd:attribute>
       </xsd:extension>
      </xsd:complexContent>
     </xsd:complexType>
    </xsd:element>
    <xsd:element ref="MIME_INFO" minOccurs="0"/>
   </xsd:sequence>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="SUPPLIER_ALT_PID">
  <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:maxLength value="50"/>
    <xsd:minLength value="1"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="SUPPLIER_ID" type="typePARTY_ID"/>
 <xsd:element name="SUPPLIER_NAME">
  <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:maxLength value="50"/>
    <xsd:minLength value="1"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="SUPPLIER_PID">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:extension base="typeSUPPLIER_PID">
     <xsd:attribute name="type" use="optional">
      <xsd:simpleType>
       <xsd:restriction base="dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="50"/>
        <xsd:pattern value="buyer_specific|ean|gtin|supplier_specific|upc|\w{1,50}"/>
       </xsd:restriction>
      </xsd:simpleType>
     </xsd:attribute>
    </xsd:extension>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="SYNONYM">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:maxLength value="80"/>
     <xsd:minLength value="1"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="T_NEW_CATALOG">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element name="PRODUCT" maxOccurs="unbounded">
     <xsd:complexType>
      <xsd:sequence>
       <xsd:element ref="SUPPLIER_PID"/>
       <xsd:element ref="PRODUCT_DETAILS"/>
       <xsd:element ref="PRODUCT_FEATURES" maxOccurs="unbounded"/>
       <xsd:element ref="PRODUCT_ORDER_DETAILS"/>
       <xsd:element ref="PRODUCT_PRICE_DETAILS" maxOccurs="unbounded"/>
       <xsd:element ref="MIME_INFO" minOccurs="0"/>
       <xsd:element name="USER_DEFINED_EXTENSIONS"/>
       <xsd:element ref="PRODUCT_REFERENCE" minOccurs="0" maxOccurs="unbounded"/>
       <xsd:element ref="PRODUCT_LOGISTIC_DETAILS" minOccurs="0"/>
      </xsd:sequence>
      <xsd:attribute name="mode" use="optional" default="new">
       <xsd:simpleType>
        <xsd:restriction base="dtSTRING">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="20"/>
         <xsd:enumeration value="new"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:attribute>
     </xsd:complexType>
    </xsd:element>
   </xsd:sequence>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="T_UPDATE_PRICES">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element name="PRODUCT" maxOccurs="unbounded">
     <xsd:complexType>
      <xsd:sequence>
       <xsd:element ref="SUPPLIER_PID"/>
       <xsd:element ref="PRODUCT_PRICE_DETAILS" maxOccurs="unbounded"/>
       <xsd:element ref="USER_DEFINED_EXTENSIONS" minOccurs="0"/>
      </xsd:sequence>
      <xsd:attribute name="mode" use="optional" default="update">
       <xsd:simpleType>
        <xsd:restriction base="dtSTRING">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="20"/>
         <xsd:enumeration value="update"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:attribute>
     </xsd:complexType>
    </xsd:element>
   </xsd:sequence>  
   <xsd:attribute name="prev_version" type="dtINTEGER" use="required"/>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="T_UPDATE_PRODUCTS">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element name="PRODUCT" maxOccurs="unbounded">
     <xsd:complexType>
      <xsd:sequence>
       <xsd:element ref="SUPPLIER_PID"/>
       <xsd:element ref="PRODUCT_DETAILS"/>
       <xsd:element ref="PRODUCT_FEATURES" minOccurs="0" maxOccurs="unbounded"/>
       <xsd:element ref="PRODUCT_ORDER_DETAILS"/>
       <xsd:element ref="PRODUCT_PRICE_DETAILS" maxOccurs="unbounded"/>
       <xsd:element ref="MIME_INFO" minOccurs="0"/>
       <xsd:element ref="USER_DEFINED_EXTENSIONS" minOccurs="0"/>
       <xsd:element ref="PRODUCT_REFERENCE" minOccurs="0" maxOccurs="unbounded"/>
       <xsd:element ref="PRODUCT_LOGISTIC_DETAILS" minOccurs="0"/>
      </xsd:sequence>
      <xsd:attribute name="mode" use="required">
       <xsd:simpleType>
        <xsd:restriction base="dtSTRING">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="20"/>
         <xsd:enumeration value="delete"/>
         <xsd:enumeration value="new"/>
         <xsd:enumeration value="update"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:attribute>
     </xsd:complexType>
    </xsd:element>
   </xsd:sequence>
   <xsd:attribute name="prev_version" type="dtINTEGER" use="required"/>
  </xsd:complexType>
 </xsd:element> 
 <xsd:element name="TAX" type="dtNUMBER"/>
 <xsd:element name="TERRITORIES">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element ref="TERRITORY" maxOccurs="unbounded"/>
   </xsd:sequence>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="TERRITORY" type="dtCOUNTRIES"/>
 <xsd:element name="TIME" type="dtTIMETYPE"/>
 <xsd:element name="TIME_BASE">
  <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:minLength value="1"/>
    <xsd:maxLength value="20"/>
    <xsd:enumeration value="date"/>
    <xsd:enumeration value="datetime"/>
    <xsd:enumeration value="dayofmonth"/>
    <xsd:enumeration value="dayofweek"/>
    <xsd:enumeration value="halfday"/>
    <xsd:enumeration value="halfofyear"/>
    <xsd:enumeration value="hour"/>
    <xsd:enumeration value="month"/>
    <xsd:enumeration value="quarterofyear"/>
    <xsd:enumeration value="time"/>
    <xsd:enumeration value="week"/>
    <xsd:enumeration value="year"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="TIME_VALUE_DURATION">
  <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:minLength value="1"/>
    <xsd:maxLength value="20"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="TIME_VALUE_END">
  <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:minLength value="1"/>
    <xsd:maxLength value="50"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="TIME_VALUE_INTERVAL" default="1">
  <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:minLength value="1"/>
    <xsd:maxLength value="20"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="TIME_VALUE_START">
  <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:minLength value="1"/>
    <xsd:maxLength value="50"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="TIMEZONE" type="dtTIMEZONETYPE"/>
 <xsd:element name="URL">
  <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:maxLength value="255"/>
    <xsd:minLength value="1"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="USER_DEFINED_EXTENSIONS" type="udxPRODUCT"/>
 <xsd:element name="VAT_ID">
  <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:maxLength value="50"/>
    <xsd:minLength value="1"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="VERSION">
  <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:maxLength value="20"/>
    <xsd:minLength value="1"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="VOLUME" type="dtNUMBER"/>
 <xsd:element name="WEIGHT" type="dtNUMBER"/>
 <xsd:element name="WIDTH" type="dtNUMBER"/>
 <xsd:element name="ZIP">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:maxLength value="20"/>
     <xsd:minLength value="1"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="UDX.EDXF.ADDITIONAL_FACTOR_INFO">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:maxLength value="150"/>
     <xsd:minLength value="1"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="UDX.EDXF.AT.REVERSE_CHARGE_INFO" type="dtBOOLEAN"/>
 <xsd:element name="UDX.EDXF.ADDITIONAL_FACTORS">
  <xsd:complexType>
   <xsd:sequence> 
    <xsd:element name="UDX.EDXF.ADDITIONAL_PRICE_FACTOR" type="dtNUMBER"/>
    <xsd:element ref="UDX.EDXF.ADDITIONAL_FACTOR_INFO" maxOccurs="unbounded"/>
   </xsd:sequence>
   </xsd:complexType> 
 </xsd:element>
 <xsd:element name="UDX.EDXF.BONUS_GROUP_SUPPLIER" type="typeSTRING00020"/>  
 <xsd:element name="UDX.EDXF.COUNTRY" type="dtCOUNTRIES"/>
 <xsd:element name="UDX.EDXF.COUNTRY_BRANCH_NUMBERS">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element name="UDX.EDXF.COUNTRY_BRANCH_NUMBER" maxOccurs="unbounded">
     <xsd:complexType>
      <xsd:simpleContent>
       <xsd:extension base="dtNUMBER">
        <xsd:attribute name="type" use="required">
         <xsd:simpleType>
          <xsd:restriction base="dtSTRING">
           <xsd:minLength value="1"/>
           <xsd:maxLength value="50"/>
           <xsd:pattern value="electrical|HVAC|building|guarantee|shipbuilding"/>
          </xsd:restriction>
         </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="country" type="dtCOUNTRIES" use="required" />
       </xsd:extension>
      </xsd:simpleContent>
     </xsd:complexType>
    </xsd:element>
   </xsd:sequence>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="UDX.EDXF.COUNTRY_BRANCH_SUPPLIER_IDS">
  <xsd:complexType>
    <xsd:sequence>
     <xsd:element name="UDX.EDXF.COUNTRY_BRANCH_SUPPLIER_ID" maxOccurs="unbounded">
      <xsd:complexType>
        <xsd:simpleContent>
         <xsd:extension base="dtNUMBER">
          <xsd:attribute name="type" use="required">
           <xsd:simpleType>
            <xsd:restriction base="dtSTRING">
             <xsd:minLength value="1"/>
             <xsd:maxLength value="50"/>
             <xsd:pattern value="electrical|HVAC|building|guarantee|shipbuilding"/>
            </xsd:restriction>
           </xsd:simpleType>
          </xsd:attribute>
         <xsd:attribute name="country" type="dtCOUNTRIES" use="required" />
        </xsd:extension>
       </xsd:simpleContent>
      </xsd:complexType>
     </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="UDX.EDXF.DECLARATION">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:extension base="typeSTRING00050">
     <xsd:attribute name="type" use="required">
      <xsd:simpleType>
       <xsd:restriction base="dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="20"/>
        <xsd:pattern value="RoHS|DOP|DOC CE|guarantee|\w{1,20}"/>
       </xsd:restriction>
      </xsd:simpleType>
     </xsd:attribute>
     <xsd:attribute name="date" type="dtDATETYPE"/>
    </xsd:extension>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element> 
 <xsd:element name="UDX.EDXF.DESCRIPTION_VERY_SHORT">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:maxLength value="40"/>
     <xsd:minLength value="1"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="UDX.EDXF.DISCOUNT_GROUP">
  <xsd:complexType>
   <xsd:choice> <!-- first or second or both of them -->
    <xsd:sequence>
      <xsd:element ref="UDX.EDXF.DISCOUNT_GROUP_MANUFACTURER"/>
      <xsd:element ref="UDX.EDXF.DISCOUNT_GROUP_SUPPLIER" minOccurs="0"/>
    </xsd:sequence>
    <xsd:element ref="UDX.EDXF.DISCOUNT_GROUP_SUPPLIER"/>
   </xsd:choice>
  </xsd:complexType> 
 </xsd:element> 
 <xsd:element name="UDX.EDXF.DISCOUNT_GROUP_MANUFACTURER">
  <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:maxLength value="10"/>
    <xsd:minLength value="1"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="UDX.EDXF.DISCOUNT_GROUP_SUPPLIER">
   <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:maxLength value="10"/>
    <xsd:minLength value="1"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="UDX.EDXF.EXPIRATION_DATE" type="dtDATETYPE"/>
 <xsd:element name="UDX.EDXF.GTIN">
  <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:maxLength value="14"/>
    <xsd:minLength value="1"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element> 
 <xsd:element name="UDX.EDXF.GS1_128">
  <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:maxLength value="48"/>
    <xsd:minLength value="1"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="UDX.EDXF.MANUFACTURER_ACRONYM"> 
   <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:maxLength value="10"/>
    <xsd:minLength value="1"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="UDX.EDXF.MATERIAL_BASIS_SURCHARGE_TABLE">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:maxLength value="50"/>
     <xsd:minLength value="1"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element> 
 <xsd:element name="UDX.EDXF.PACKING_UNIT">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element name="UDX.EDXF.QUANTITY_MIN" type="dtFLOAT"/>
    <xsd:element name="UDX.EDXF.QUANTITY_MAX" type="dtFLOAT" minOccurs="0"/>
    <xsd:element name="UDX.EDXF.PACKING_UNIT_CODE" type="dtPUNIT"/>
    <xsd:element ref="UDX.EDXF.PACKING_UNIT_NAME" minOccurs="0" maxOccurs="unbounded"/>
    <xsd:element name="UDX.EDXF.PACKAGE_BREAK" type="dtBOOLEAN" minOccurs="0"/>
    <xsd:element name="UDX.EDXF.VOLUME" type="dtNUMBER" minOccurs="0"/>
    <xsd:element name="UDX.EDXF.WEIGHT" type="dtNUMBER" minOccurs="0"/>
    <xsd:element name="UDX.EDXF.LENGTH" type="dtNUMBER" minOccurs="0"/>
    <xsd:element name="UDX.EDXF.WIDTH" type="dtNUMBER" minOccurs="0"/>
    <xsd:element name="UDX.EDXF.DEPTH" type="dtNUMBER" minOccurs="0"/>
    <xsd:element name="UDX.EDXF.DIAMETER" type="dtNUMBER" minOccurs="0"/>
    <xsd:element ref="UDX.EDXF.GTIN" minOccurs="0"/>
    <xsd:element ref="UDX.EDXF.GS1_128" minOccurs="0"/>    
   </xsd:sequence>
  </xsd:complexType> 
 </xsd:element> 
 <xsd:element name="UDX.EDXF.PACKING_UNIT_NAME">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:minLength value="1"/>
     <xsd:maxLength value="20"/>
     <xsd:enumeration value="f-pak"/>
     <xsd:enumeration value="m-pak"/>
     <xsd:enumeration value="d-pak"/>
     <xsd:enumeration value="t-pak"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element> 
 <xsd:element name="UDX.EDXF.PACKING_UNITS">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element ref="UDX.EDXF.PACKING_UNIT" maxOccurs="unbounded"/>
   </xsd:sequence>
  </xsd:complexType> 
 </xsd:element>
 <xsd:element name="UDX.EDXF.PKWIU">
  <xsd:simpleType>
   <xsd:restriction base="dtSTRING">
    <xsd:maxLength value="50"/>
    <xsd:minLength value="1"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="UDX.EDXF.PRODUCT_LOGISTIC_DETAILS">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element name="UDX.EDXF.NETWEIGHT" type="dtNUMBER" minOccurs="0"/>
    <xsd:element name="UDX.EDXF.REGION_OF_ORIGIN" type="dtSTRING" minOccurs="0"/>
   </xsd:sequence>
   </xsd:complexType> 
 </xsd:element> 
 <xsd:element name="UDX.EDXF.PRODUCT_SERIES">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:maxLength value="50"/>
     <xsd:minLength value="1"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="UDX.EDXF.PRODUCT_TO_STOCK" type="dtBOOLEAN"/> 
 <xsd:element name="UDX.EDXF.PRODUCT_VARIATION">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:maxLength value="50"/>
     <xsd:minLength value="1"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element> 
 <xsd:element name="UDX.EDXF.REACH">
  <xsd:complexType>
   <xsd:sequence> 
    <xsd:element name="UDX.EDXF.REACH.LISTDATE" type="dtDATETYPE" minOccurs="0"/>
    <xsd:element ref="UDX.EDXF.REACH.INFO"/>
   </xsd:sequence>
   </xsd:complexType> 
 </xsd:element> 
 <xsd:element name="UDX.EDXF.REACH.INFO">
  <xsd:simpleType>
   <xsd:restriction base="xsd:string">
    <xsd:pattern value="[Ff][Aa][Ll][Ss][Ee]|[Tt][Rr][Uu][Ee]|[Nn][Oo][ ][Dd][Aa][Tt][Aa]"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>  
 <xsd:element name="UDX.EDXF.SURCHARGE">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element name="UDX.EDXF.SURCHARGE_TYPE" type="typeSTRING00020"/>
    <xsd:element ref="UDX.EDXF.SURCHARGE_MANNER"/>
    <xsd:choice minOccurs="0">
     <xsd:element name="UDX.EDXF.SURCHARGE_PERCENTAGE" type="dtNUMBER"/>
     <xsd:element name="UDX.EDXF.SURCHARGE_PRICE_AMOUNT" type="dtNUMBER"/>    
    </xsd:choice>
    <xsd:element name="UDX.EDXF.SURCHARGE_CALCULATION" type="dtNUMBER" minOccurs="0"/>
    <xsd:element name="UDX.EDXF.MATERIAL_BASIS" type="dtNUMBER" minOccurs="0"/>
    <xsd:choice minOccurs="0">
     <xsd:element name="UDX.EDXF.MATERIAL_BASIS_WEIGHT" type="dtNUMBER"/>
     <xsd:sequence>
      <xsd:element name="UDX.EDXF.MATERIAL_BASIS_SURCHARGE_THRESHOLD" type="dtNUMBER"/>
      <xsd:element name="UDX.EDXF.MATERIAL_BASIS_SURCHARGE_SHUTTER" type="dtINTEGER" minOccurs="0"/>
      <xsd:element name="UDX.EDXF.MATERIAL_BASIS_SURCHARGE_CREDIT" type="dtINTEGER" minOccurs="0"/>
      <xsd:element ref="UDX.EDXF.MATERIAL_BASIS_SURCHARGE_TABLE" minOccurs="0" maxOccurs="unbounded"/>
     </xsd:sequence>
    </xsd:choice> 
   </xsd:sequence> 
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="UDX.EDXF.SURCHARGE_LIST">
  <xsd:complexType>
   <xsd:sequence>
    <xsd:element ref="UDX.EDXF.SURCHARGE" minOccurs="1" maxOccurs="unbounded"/>
   </xsd:sequence>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="UDX.EDXF.SURCHARGE_MANNER">
  <xsd:simpleType>
   <xsd:restriction base="xsd:string">
    <xsd:pattern value="[Bb][Aa][Ss][Ee]|[Cc][Uu][Mm][Uu][Ll][Aa][Tt][Ee][Dd]"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <xsd:element name="UDX.EDXF.TENDER_TEXT">
  <xsd:complexType>
   <xsd:simpleContent>
    <xsd:restriction base="dtMLSTRING">
     <xsd:maxLength value="64000"/>
     <xsd:minLength value="1"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>
 </xsd:element>
 <xsd:element name="UDF.EDXF.VALID_FROM" type="dtDATETYPE"/>
 <xsd:element name="UDX.EDXF.VERSION" type="typeSTRING00020"/>
</xsd:schema>